FILENAME=`basename "$0"`
echo "$FILENAME"
TMP_FILE="./tmp1"
TARGET_FILE="./target"
cat $FILENAME > $TARGET_FILE
FILESIZE=0
until [ $FILESIZE -gt 2048 ]; do
  cp $TARGET_FILE $TMP_FILE
  cat $TMP_FILE >> $TARGET_FILE
  FILESIZE=`du $TARGET_FILE | awk '{print $1}'`
  echo " size of file: $FILESIZE bytes"
  sleep 1
done
echo "new size of file exceeded target of 2kb plus"#!/bin/bash
# this script will loop the condition until it makes the file size greater thab 2048
FILENAME=`basename "$0"`
echo "$FILENAME"
TMP_FILE="./tmp1"
TARGET_FILE="./target"
cat $FILENAME > $TARGET_FILE
FILESIZE=0
until [ $FILESIZE -gt 2048 ]; do
  cp $TARGET_FILE $TMP_FILE
  cat $TMP_FILE >> $TARGET_FILE
  FILESIZE=`du $TARGET_FILE | awk '{print $1}'`
  echo " size of file: $FILESIZE bytes"
  sleep 1
done
